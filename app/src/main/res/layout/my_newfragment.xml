<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <com.haotang.petworker.view.ObservableScrollView
        android:id="@+id/osv_myfragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/iv_myfragment_top"
                android:layout_width="match_parent"
                android:layout_height="239dp"
                android:scaleType="fitXY"
                android:src="@drawable/bg_mytop_icon" />

            <RelativeLayout
                android:id="@+id/rv_myfragment_top"
                android:layout_width="match_parent"
                android:layout_height="145dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="119dp"
                android:layout_marginRight="15dp"
                android:background="@drawable/bg_white_round20">

                <TextView
                    android:id="@+id/tv_jobnumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="15dp"
                    android:text="工号  "
                    android:textColor="#FF9FA7B9"
                    android:textSize="15sp" />
            <RelativeLayout
                android:id="@+id/rl_my_top"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="45.5dp">
                <TextView
                    android:id="@+id/tv_worker_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="李皮皮"
                    android:textColor="#FF002241"
                    android:textSize="24sp" />

                <ImageView
                    android:id="@+id/iv_myfragment_level"
                    android:layout_width="79dp"
                    android:layout_height="18dp"
                    android:layout_marginLeft="11.5dp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/tv_worker_name"
                    android:scaleType="fitXY"
 />

            </RelativeLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/rl_my_top"
                    android:layout_marginTop="12dp"
                    android:background="@drawable/bg_gray_bottomround">

                    <TextView
                        android:id="@+id/tv_ordernum_tip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:text="上线接单数"
                        android:textColor="#FF717985"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/tv_order_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="12dp"
                        android:layout_toRightOf="@+id/tv_ordernum_tip"
                        android:textColor="#FF384359"
                        android:textSize="20sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/tv_feedback_num"
                        android:text="好评率"
                        android:textColor="#FF717985"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/tv_feedback_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="15dp"
                        android:textColor="#FF384359"
                        android:textSize="20sp" />
                </RelativeLayout>
            </RelativeLayout>

            <com.haotang.petworker.view.NiceImageView
                android:id="@+id/iv_myfragment_head"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="86dp"
                app:corner_radius="@dimen/dp_25"
                android:scaleType="fitXY"
                android:src="@drawable/logo" />
            <LinearLayout
                android:id="@+id/ll_myfragment_middle"
                android:layout_width="match_parent"
                android:layout_height="108dp"
                android:layout_below="@+id/rv_myfragment_top"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/dp_15"
                android:paddingRight="@dimen/dp_15"
                android:paddingBottom="@dimen/dp_20"
                android:background="@drawable/bg_my_middle"
                >
                <LinearLayout
                    android:id="@+id/ll_myfragment_production"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="48.5dp"
                        android:layout_height="48.5dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_my_productionnew"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="我的作品"
                        android:textColor="#FF717985"
                        android:textSize="15sp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_myfragment_evaluate"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="48.5dp"
                        android:layout_height="48.5dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_my_evaluate"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="我的评价"
                        android:textColor="#FF717985"
                        android:textSize="15sp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_myfragment_level"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="48.5dp"
                        android:layout_height="48.5dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_my_level"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="我的等级"
                        android:textColor="#FF717985"
                        android:textSize="15sp"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_myfragment_bottom"
                android:layout_width="fill_parent"
                android:layout_height="378dp"
                android:orientation="vertical"
                android:layout_below="@+id/ll_myfragment_middle"
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:background="@drawable/bg_white_round20"
                >
                <LinearLayout
                    android:id="@+id/ll_myfragment_ordertime"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_online_time"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="上线接单时间查询"
                        android:textColor="#FF717985"
                        android:textSize="16sp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="right">
                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="13dp"
                            android:src="@drawable/icon_jiantou_right"
                            android:scaleType="fitXY"
                            android:layout_gravity="right"
                            android:layout_marginRight="@dimen/dp_15"
                            />

                    </LinearLayout>


                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_myfragment_servicetime"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_service_time"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="服务时长查询"
                        android:textColor="#FF717985"
                        android:textSize="16sp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="right">
                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="13dp"
                            android:src="@drawable/icon_jiantou_right"
                            android:scaleType="fitXY"
                            android:layout_gravity="right"
                            android:layout_marginRight="@dimen/dp_15"
                            />

                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_myfragment_eattime"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_seteattime"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="吃饭时间设置"
                        android:textColor="#FF717985"
                        android:textSize="16sp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="right">
                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="13dp"
                            android:src="@drawable/icon_jiantou_right"
                            android:scaleType="fitXY"
                            android:layout_gravity="right"
                            android:layout_marginRight="@dimen/dp_15"
                            />

                    </LinearLayout>


                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_myfragment_areasug"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_area_suggest"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="区域建议"
                        android:textColor="#FF717985"
                        android:textSize="16sp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:orientation="horizontal"
                        android:gravity="right">
                        
                        <View
                            android:id="@+id/v_red_one"
                            android:layout_width="5dp"
                            android:layout_height="5dp"
                            android:background="@drawable/bg_red_circle"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="10dp"
                            android:visibility="gone"
                            />
                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="13dp"
                            android:src="@drawable/icon_jiantou_right"
                            android:scaleType="fitXY"
                            android:layout_gravity="right"
                            android:layout_marginRight="@dimen/dp_15"
                            />

                    </LinearLayout>


                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_myfragment_evaluateboss"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_myfragment_evaluate"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="评价店长"
                        android:textColor="#FF717985"
                        android:textSize="16sp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="right">
                        <View
                            android:id="@+id/v_red_two"
                            android:layout_width="5dp"
                            android:layout_height="5dp"
                            android:background="@drawable/bg_red_circle"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="10dp"
                            android:visibility="gone"
                            />
                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="13dp"
                            android:src="@drawable/icon_jiantou_right"
                            android:scaleType="fitXY"
                            android:layout_gravity="right"
                            android:layout_marginRight="@dimen/dp_15"
                            />

                    </LinearLayout>


                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_myfragment_beboss"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_employ"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="应聘店长"
                        android:textColor="#FF717985"
                        android:textSize="16sp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="right">
                        <View
                            android:id="@+id/v_red_three"
                            android:layout_width="5dp"
                            android:layout_height="5dp"
                            android:background="@drawable/bg_red_circle"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="10dp"
                            android:visibility="gone"
                            />
                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="13dp"
                            android:src="@drawable/icon_jiantou_right"
                            android:scaleType="fitXY"
                            android:layout_gravity="right"
                            android:layout_marginRight="@dimen/dp_15"
                            />

                    </LinearLayout>


                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_myfragment_study"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_study_farmland"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="学习园地"
                        android:textColor="#FF717985"
                        android:textSize="16sp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="right">
                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="13dp"
                            android:src="@drawable/icon_jiantou_right"
                            android:scaleType="fitXY"
                            android:layout_gravity="right"
                            android:layout_marginRight="@dimen/dp_15"
                            />

                    </LinearLayout>


                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_myfragment_about"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_about_app"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="关于"
                        android:textColor="#FF717985"
                        android:textSize="16sp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        />
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:gravity="right">
                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="13dp"
                            android:src="@drawable/icon_jiantou_right"
                            android:scaleType="fitXY"
                            android:layout_gravity="right"
                            android:layout_marginRight="@dimen/dp_15"
                            />

                    </LinearLayout>


                </LinearLayout>
            </LinearLayout>
            <Button
                android:id="@+id/btn_quite"
                android:background="@drawable/bg_quite_gray"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginRight="15dp"
                android:layout_marginLeft="15dp"
                android:text="退出登录"
                android:textColor="#FF002241"
                android:textSize="18sp"
                android:layout_below="@+id/ll_myfragment_bottom"
                />
        </RelativeLayout>

    </com.haotang.petworker.view.ObservableScrollView>>
    <include
        android:id="@+id/rl_myfragment_title"
        layout="@layout/titlebaraphl" />
</RelativeLayout>